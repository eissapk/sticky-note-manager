{
  "name": "Note Manager",
  "version": "1.0.0",
  "private": true,
  "description": "Note Manager that works offline using your flash drive",
  "main": "dist/index.html",
  "scripts": {
  "build":"cd src && npm run build",
  "dev":"cd src && npm run dev"
  },
  "window": {
    "title": "Note Manager",
    "toolbar": true,
    "frame": true,
    "width": 1073,
    "height": 660,
    "min_width": 500,
    "min_height": 500,
    "icon": "dist/vite.svg",
    "position": "center",
    "background-color": "#a69aa6"
  },
  "author": "Eissa Saber",
  "license": "MIT",
  "chromium-args": "--disable-web-security",
  "webview": {
    "partitions": [
      {
        "name": "trusted",
        "accessible_resources": [
          "file://*/*"
        ]
      }
    ]
  },
  "dependencies": {}
}
